{
  "name": "todo",
  "version": "1.0.0",
  "description": "A ui5-based PWA demo",
  "main": "index.js",
  "scripts": {
    "start": "ui5 serve",
    "start-dist": "firebase serve",
    "build": "ui5 build --cleanDest self-contained --all",
    "build-nopwa": "ui5 build --cleanDest --dest dist_nopwa self-contained --all --config ui5-nopwa.yaml"
  },
  "author": "James Hale",
  "license": "Apache-2.0",
  "devDependencies": {
    "@ui5/cli": "^2.4.1",
    "ui5-middleware-livereload": "^0.4.1",
    "ui5-task-pwa-enabler": "^0.2.2"
  },
  "ui5": {
    "dependencies": [
      "ui5-middleware-livereload",
      "ui5-task-pwa-enabler"
    ]
  }
}